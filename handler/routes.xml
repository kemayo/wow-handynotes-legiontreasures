<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
    <Script file="routes.lua"/>
    <Frame name="HandyNotes_LegionTreasuresRoutePinTemplate" mixin="HandyNotes_LegionTreasuresRoutePinMixin" enableMouse="true" virtual="true">
        <Size x="40" y="40"/>
        <Layers>
            <Layer level="BACKGROUND">
                <Texture parentKey="Icon"/>
            </Layer>
            <Layer level="BORDER">
                <Texture parentKey="IconBorder"/>
            </Layer>
            <Layer level="OVERLAY">
                <Texture parentKey="IconDisabledOverlay" hidden="true"/>
            </Layer>
        </Layers>
    </Frame>
    <Frame name="HandyNotes_LegionTreasuresRoutePinConnectionTemplate" mixin="HandyNotes_LegionTreasuresRoutePinConnectionMixin" ignoreParentScale="true" virtual="true">
        <Size x="64" y="64"/>
        <Layers>
            <Layer level="BACKGROUND">
                <Line parentKey="Line" />
            </Layer>
        </Layers>
    </Frame>
</Ui>